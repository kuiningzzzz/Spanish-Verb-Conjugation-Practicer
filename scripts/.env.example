# ===== prompt_matrix_test.js（生成题+验证测试）=====
# DeepSeek 配置
DEEPSEEK_API_KEY=
DEEPSEEK_API_URL=https://api.deepseek.com/v1/chat/completions

# Qwen 配置（二选一：QWEN_API_KEY 或 DASHSCOPE_API_KEY）
QWEN_API_KEY=
DASHSCOPE_API_KEY=
QWEN_API_URL=https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions

# 模型列表（逗号分隔，支持 provider:model 格式）
MODELS=deepseek:deepseek-chat,qwen:qwen-plus,qwen:qwen3-max

# 生成温度（逗号分隔，只用于生成）
GENERATOR_TEMPERATURES=0.7

# 是否使用 validator（true/false）
USE_VALIDATOR=true

# prompt 版本（逗号分隔，index 从 0 开始）
GENERATOR_PROMPTS=0
VALIDATOR_PROMPTS=0
REVISOR_PROMPTS=0

# validator 模型温度（单值，默认沿用生成温度）
VALIDATOR_TEMPERATURE=
# revisor 模型温度（单值，推荐 0.4）
REVISOR_TEMPERATURE=0.4

# 跑测试的次数（每次随机一个动词+一个时态+一个人称）
TEST_CASES=1

# 输出 CSV 到文件（不设置则只输出到终端）
OUTPUT_FILE=

# 日志输出到文件（不设置则只打印到终端）
LOG_FILE=
 
# 生成最大 tokens（单值，默认 1000）
GENERATOR_MAX_TOKENS=1000 
VALIDATOR_MAX_TOKENS=700 
REVISOR_MAX_TOKENS=500

# 是否启用 revisor（true/false）
USE_REVISOR=true

# 请求超时时间（毫秒）
REQUEST_TIMEOUT_MS=45000 

# ===== prompt_conj-with-pronoun_test.js（动词+代词组合实验）=====
# 说明：
# - 仍复用上面的 API 配置：DEEPSEEK_* / QWEN_*。
# - 下列未设置时，部分参数会回退到通用参数：
#   CONJ_WITH_PRONOUN_*_PROMPTS -> GENERATOR_PROMPTS / VALIDATOR_PROMPTS / REVISOR_PROMPTS
#   CONJ_WITH_PRONOUN_*_MAX_TOKENS -> GENERATOR_MAX_TOKENS / VALIDATOR_MAX_TOKENS / REVISOR_MAX_TOKENS
#   CONJ_WITH_PRONOUN_VALIDATOR_TEMPERATURE -> VALIDATOR_TEMPERATURE
#   CONJ_WITH_PRONOUN_REVISOR_TEMPERATURE -> REVISOR_TEMPERATURE
# - TEST_CASES / MODELS / GENERATOR_TEMPERATURES / USE_VALIDATOR / USE_REVISOR 直接复用通用参数。

# prompt 版本（逗号分隔，index 从 0 开始；当前三个文件默认只有 0）
CONJ_WITH_PRONOUN_GENERATOR_PROMPTS=0
CONJ_WITH_PRONOUN_VALIDATOR_PROMPTS=0
CONJ_WITH_PRONOUN_REVISOR_PROMPTS=0

# host_form 候选（逗号分隔，可选：finite,imperative,infinitive,gerund,prnl）
CONJ_WITH_PRONOUN_HOST_FORMS=finite,imperative,infinitive,gerund,prnl

# finite 时态候选（逗号分隔，可选：present,preterite,imperfect）
CONJ_WITH_PRONOUN_FINITE_TENSES=present,preterite,imperfect

# validator/revisor 温度（单值，不设则按上面的回退规则）
CONJ_WITH_PRONOUN_VALIDATOR_TEMPERATURE=
CONJ_WITH_PRONOUN_REVISOR_TEMPERATURE=

# 生成最大 tokens（单值，不设则按上面的回退规则）
CONJ_WITH_PRONOUN_GENERATOR_MAX_TOKENS=1300
CONJ_WITH_PRONOUN_VALIDATOR_MAX_TOKENS=900
CONJ_WITH_PRONOUN_REVISOR_MAX_TOKENS=700

# 输出路径（建议使用 scripts/output 下文件）
CONJ_WITH_PRONOUN_OUTPUT_FILE=./output/prompt_conj-with-pronoun.csv
CONJ_WITH_PRONOUN_LOG_FILE=./output/prompt_conj-with-pronoun_logs.csv

# ===== get_verb.py（动词变位生成）=====
# Qwen 模型
VERB_GENERATEION_MODEL=qwen-plus
